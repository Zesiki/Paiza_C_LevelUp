using System;

class Program
{
    static void Main()
    {
       int num = int.Parse(Console.ReadLine());
       string [] data = Console.ReadLine().Split(' ');
       int answer = 0;
       
       for(var i = 0; i<num; i++)
       {
           if (int.Parse(data[i]) % 3 == 0)
           {
               answer = answer + 1;
           } else
           {
               answer = answer;
           }
       }
       Console.WriteLine(answer);
    }
}
